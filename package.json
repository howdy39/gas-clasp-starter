{
  "name": "gas-clasp-starter",
  "version": "2.0.0",
  "description": "gas-clasp-starter project.",
  "homepage": "https://github.com/howdy39/gas-clasp-starter",
  "main": "build/index.js",
  "license": "MIT",
  "keywords": [
    "gas",
    "google-apps-script",
    "clasp"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint --fix src/**/*.ts",
    "test": "jest",
    "bundle": "rollup --no-treeshake -c rollup.config.mjs",
    "build": "npm run lint && npm run test && npm run clean && npm run bundle && cpx src/appsscript.json dist",
    "push": "npm run build && clasp push"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/google-apps-script": "^1.0.77",
    "@types/jest": "^29.5.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "cpx": "^1.5.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "gts": "^5.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "rollup": "^4.3.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-license": "^3.2.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(src/.+(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
